    // Get the solar date according the lunar date ï¼ˆthe date must years between 1900 and 2099)
    // @param lunarDate, format: 2014-02-21
    // @param leap, true means the lunar month is leap
    public static Date convertLunarToSolar(String lunarDate, Boolean isLeap, Boolean ignoreBigMonth)
    {
        List<String> lunar = lunarDate.split('-');
        Integer lunarYear = Integer.valueOf(lunar[0]);
        Integer lunarMonth = Integer.valueOf(lunar[1]);
        Integer lunarDay = Integer.valueOf(lunar[2]);

        Integer yearIndex = lunarYear - MIN_YEAR;
        Integer monthIndex = lunarMonth;

        if(yearIndex < 0 || yearIndex > DATAS.size())
        {
            throw new ChineseCalendarException('The date is out of range.');
        }

        // DATA[index][0] indicates the leap month of one year
        Integer leapMonth = Integer.valueOf(DATAS[yearIndex][0]);
        if(((leapMonth != 0) && (lunarMonth > leapMonth)) ||
            (isLeap && lunarMonth == leapMonth))
        {
            monthIndex++;
        }

        Date solar = getSolarDateByLunarFirstDay(yearIndex, monthIndex, lunarYear);

        // Check the input lunar date whether it is big month (30 days)
        Date nextSolar = getSolarDateByNextLunarFirstDay(yearIndex, monthIndex, lunarYear, leapMonth);
        Integer oneMonthDays = solar.daysBetween(nextSolar);

        System.debug('oneMonthDays:' + oneMonthDays + ';nextSolar' + nextSolar + ';' + solar);

        if(lunarDay > oneMonthDays)
        {
            if(ignoreBigMonth) // if input lunar date (30) is not exit in this year, that can change to 29
            {
                lunarDay = 29;
            }
            else
            {
                throw new ChineseCalendarException('The date is invalid.');
            }
        }

        return solar.addDays(lunarDay-1);
    }

    public static Date convertLunarToSolar(String lunarDate)
    {
        return convertLunarToSolar(lunarDate, false, true);
    }

    // Gets first day of lunar month map a solar date.
    // e.g. 1222 12 is month, 22 is a day
    private static Date getSolarDateByLunarFirstDay(Integer yearIndex, Integer monthIndex, Integer lunarYear)
    {
        String solarDate = DATAS[yearIndex][monthIndex];
        Integer solarMonth = Integer.valueOf(solarDate.substring(0, 2));
        Integer solarDay = Integer.valueOf(solarDate.substring(2, 4));
        Integer solarYear = lunarYear;

        if(solarMonth > 12)
        {
            solarYear++;
            solarMonth -= 12;
        }

        return Date.parse(solarMonth + '/' + solarDay + '/' + solarYear);
    }

    private static Date getSolarDateByNextLunarFirstDay(Integer yearIndex, Integer monthIndex, Integer lunarYear, Integer leapMonth)
    {
        if((leapMonth == 0 && monthIndex == 12) || (leapMonth != 0 && monthIndex == 13))
        {
            yearIndex++;
            lunarYear++;
            monthIndex = 1;
        }
        else
        {
            monthIndex++;
        }

        return getSolarDateByLunarFirstDay(yearIndex, monthIndex, lunarYear);
    }

    public class ChineseCalendarException extends Exception {}
