    <validationRules>
        <fullName>TimeCardCannotBeLoggedForOtherPerson</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TeamMember__r.User__c !=  $User.Id ,
CONTAINS( $Profile.Name , &quot;Developer&quot;), ISNEW()
)</errorConditionFormula>
        <errorMessage>You can only create a time card for yourself.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimeCardCannotBeLoggedUnderBilledChange</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; Change__r.Billed__c</errorConditionFormula>
        <errorMessage>You can&apos;t log a time card to a billed change. Please make sure you&apos;re logging time under the right change or contact your project manager for further information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TimeCardCannotBeLoggedUnderClosedProject</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; TEXT( Project__r.Status__c ) = &quot;Closed&quot;</errorConditionFormula>
        <errorMessage>You can&apos;t log a time card to a closed projects.</errorMessage>
    </validationRules>
