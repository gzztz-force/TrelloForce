        }

        .new-page {
            background-color: #EEEEEE;
        }

        .new-page h2 {
            border-bottom: 1px solid #bbb;
            display: block;
        }

        .spaces-list li>div {
            margin-left: 30px;
            position: relative;
        }

        .spaces-list li {
            margin-top: 5px;
            padding-left: 5px;
            padding-top: 2px;
            font-size: 15px;
        }

        .spaces-list .fav {
            background-position: 0 0;
            height: 16px;
            width: 16px;
            margin: 2px 0 0 3px;
            float: left;
        }

        .spaces-list .bookmark {
            margin: 2px 10px 0 3px;
            position: absolute;
            right: 10px;
            top: 2px;
        }

        .spaces-list .divider {
            height: 1px;
            margin: 9px 1px;
            overflow: hidden;
            background-color: #e5e5e5;
            border-bottom: 1px solid #fff;
            padding: 0px;
